<h1>Menus#index</h1>
<%= link_to "新規トレーニング", new_menu_path %>
<% @menu.each do |m| %>
    <ul>
        <li><%= link_to m.name, new_menu_load_path(m.id) %></li>
        <li><%= link_to "edit", edit_menu_path(m.id) %></li>
        <li><%= link_to "delete", menu_path(m.id), method: :delete, "data-confirm" => "本当に削除しますか？" %></li>
    </ul>
<% end %>

